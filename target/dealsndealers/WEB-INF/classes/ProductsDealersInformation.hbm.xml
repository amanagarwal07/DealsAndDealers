<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.endurance.dealsndealers.productsperdealer.ProductsDealersInformation" table="products_per_dealer">
        <id name="id" column="id">
            <generator class="identity"></generator>
        </id>
        <property name="dealerId" column="dealer_id" type="java.lang.Integer"/>
        <property name="productId" column="product_id" type="java.lang.Integer"/>
        <property name="price" column="price" type="java.lang.Double"/>
    </class>
</hibernate-mapping>