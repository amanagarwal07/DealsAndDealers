<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.endurance.dealsndealers.expense.ExpenseInformation" table="expense">
        <id name="id" column="id">
            <generator class="identity"></generator>
        </id>
        <property name="receiptId" column="receipt_id" type="java.lang.Integer"/>
        <property name="smbId" column="smb_id" type="java.lang.Integer"/>
        <property name="productId" column="product_id" type="java.lang.Integer"/>
        <property name="dealerId" column="dealer_id" type="java.lang.Integer"/>
        <property name="quantity" column="quantity" type="java.lang.Double"/>
        <property name="price" column="price" type="java.lang.Double"/>
        <property name="expenseStatus" column="expense_status" type="java.lang.Integer"/>
        <property name="creationDate" column="creation_dt" type="java.lang.Integer"/>
        <property name="rating" column="rating" type="java.lang.Double"/>
    </class>
</hibernate-mapping>