<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.endurance.dealsndealers.smb.SmbInformation" table="smb">
        <id name="id" column="id">
            <generator class="identity"></generator>
        </id>
        <property name="orgName" column="org_name" type="java.lang.String"/>
        <property name="addressLine1" column="address_line1" type="java.lang.String"/>
        <property name="addressLine2" column="address_line2" type="java.lang.String"/>
        <property name="zipCode" column="zipcode" type="java.lang.Integer"/>
        <property name="city" column="city" type="java.lang.String"/>
        <property name="state" column="state" type="java.lang.String"/>
        <property name="mobileNumber" column="phone" type="java.lang.String"/>
    </class>
</hibernate-mapping>